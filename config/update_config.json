{
  "auto_update_enabled": true,
  "update_interval_hours": 24,
  "ai_optimization_enabled": true,
  "ai_cache_size": 1000,
  "ai_response_timeout": 30,
  "db_optimization_enabled": true,
  "db_vacuum_interval_hours": 168,
  "db_index_optimization": true,
  "backup_before_update": true,
  "max_backups": 10,
  "performance_monitoring": true,
  "update_sources": [
    "https://api.github.com/repos/your-repo/releases/latest"
  ],
  "excluded_files": [
    ".env",
    "config/",
    "data/",
    "logs/",
    "backups/"
  ],
  "ai_models": {
    "default": "llama3.2",
    "fallback": "llama2",
    "optimization": "llama3.2"
  }
}